{
  "workbench.colorTheme": "Default Dark Modern",
  "redhat.telemetry.enabled": false,
  "php.debug.executablePath": "",
  "editor.renderWhitespace": "trailing",
  "editor.foldingStrategy": "indentation",
  "django.snippets.use": false,
  "django.showContributeNotification": false,
  "django.i18n": false,
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "workbench.editorAssociations": {
    "*.svg": "default"
  },
  "git.enableSmartCommit": true,
  "git.confirmSync": false,
  "github.copilot.renameSuggestions.triggerAutomatically": false,
  "github.copilot.enable": {
    "*": false,
    "plaintext": false,
    "markdown": false,
    "scminput": false,
    "jsonc": false
  },
  "inlineChat.holdToSpeech": false,
  "inlineChat.lineNaturalLanguageHint": false,
  "accessibility.verbosity.inlineChat": false,
  "accessibility.verbosity.panelChat": false,
  "workbench.editor.empty.hint": "hidden",
  "workbench.startupEditor": "none",

  "cSpell.userWords": [
    "accessibilities",
    "Behaviour",
    "Dont",
    "firestore",
    "Jaydan",
    "Lumoddy",
    "Mathf",
    "nameof",
    "nodiscard",
    "noreturn",
    "Pressable",
    "Rects",
    "resizer",
    "Rigidbody",
    "springframework",
    "textonly",
    "Toggleable",
    "unshifted"
  ],

  "java.compile.nullAnalysis.mode": "automatic",

  "editor.rulers": [
    {
      "color": "#303030",
      "column": 80,
    },
    {
      "color": "#404040",
      "column": 120,
    },
  ],
  "C_Cpp.default.cppStandard": "gnu++23",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "C_Cpp.suggestSnippets": false,
  "javascript.preferences.importModuleSpecifierEnding": "js",
  "typescript.preferences.importModuleSpecifierEnding": "js",
  "editor.minimap.showSlider": "always",
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "variable.parameter",
        "settings": {
          "foreground": "#4FC1FF",
        },
      },
    ],
  },
  "terminal.integrated.persistentSessionReviveProcess": "onExitAndWindowClose",
}