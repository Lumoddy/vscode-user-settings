[
  {
    "key": "ctrl+shift+c",
    "command": "-workbench.action.terminal.openNativeConsole",
    "when": "!terminalFocus",
  },

  {
    "key": "ctrl+r ctrl+r",
    "command": "renameFile",
    "when": "explorerViewletFocus",
  },
  {
    "key": "ctrl+r ctrl+r",
    "command": "editor.action.rename",
    "when": "editorTextFocus",
  },
  {
    "key": "ctrl+r ctrl+u",
    "command": "editor.action.transformToUppercase",
    "when": "editorTextFocus",
  },
  {
    "key": "ctrl+r ctrl+l",
    "command": "editor.action.transformToLowercase",
    "when": "editorTextFocus",
  },

  {
    "key": "ctrl+insert ctrl+0",
    "command": "editor.action.insertSnippet",
    "args": {
      "snippet": "$CURSOR_INDEX",
    },
    "when": "editorTextFocus && editorHasMultipleSelections",
  },
  {
    "key": "ctrl+insert ctrl+1",
    "command": "editor.action.insertSnippet",
    "args": {
      "snippet": "$CURSOR_NUMBER",
    },
    "when": "editorTextFocus && editorHasMultipleSelections",
  },
  {
    "key": "ctrl+insert ctrl+u",
    "command": "editor.action.insertSnippet",
    "args": {
      "snippet": "$UUID",
    },
    "when": "editorTextFocus",
  },
  {
    "key": "ctrl+shift+v",
    "command": "editor.action.insertSnippet",
    "args": {
      "snippet": "${0:${CLIPBOARD}}",
    },
    "when": "editorTextFocus",
  },

  {
    "key": "ctrl+shift+s",
    "command": "saveAll",
  },

  {
    "key": "ctrl+w",
    "command": "workbench.action.closeAuxiliaryBar",
    "when": "auxiliaryBarFocus",
  },
  {
    "key": "ctrl+w",
    "command": "workbench.action.closeSidebar",
    "when": "sideBarFocus && activeEditorIsNotPreview",
  },
  {
    "key": "ctrl+w",
    "command": "workbench.action.closeActiveEditor",
    "when": "sideBarFocus && !activeEditorIsNotPreview",
  },
  {
    "key": "ctrl+w",
    "command": "workbench.action.togglePanel",
    "when": "panelFocus",
  },

  {
    "key": "ctrl+u",
    "command": "inlineChat.start",
  },
  {
    "key": "ctrl+k ctrl+o",
    "command": "editor.action.goToDeclaration",
    "when": "editorTextFocus",
  },
  {
    "key": "ctrl+k ctrl+k",
    "command": "workbench.action.focusActiveEditorGroup",
    "when": "!editorTextFocus",
  },
  {
    "key": "ctrl+k ctrl+k",
    "command": "workbench.action.navigateBackInEditLocations",
    "when": "editorTextFocus",
  },
  {
    "key": "ctrl+k ctrl+l",
    "command": "workbench.action.navigateForwardInEditLocations",
    "when": "editorTextFocus",
  },
  {
    "key": "ctrl+i",
    "command": "editor.action.triggerSuggest",
    "when": "editorTextFocus",
  },
  {
    "key": "ctrl+k ctrl+s",
    "command": "editor.action.setSelectionAnchor",
    "when": "editorTextFocus",
  },
  {
    "key": "ctrl+k ctrl+k",
    "command": "editor.action.goToSelectionAnchor",
    "when": "selectionAnchorSet",
  },
  {
    "key": "shift+ctrl+k shift+ctrl+k",
    "command": "editor.action.selectFromAnchorToCursor",
    "when": "editorTextFocus && selectionAnchorSet",
  },

  {
    "key": "ctrl+shift+u",
    "command": "workbench.panel.chat",
  },

  {
    "key": "ctrl+up",
    "command": "runCommands",
    "args": { // x8
      "commands": ["cursorUp", "cursorUp", "cursorUp", "cursorUp", "cursorUp", "cursorUp", "cursorUp", "cursorUp"],
    },
    "when": "textInputFocus",
  },
  {
    "key": "shift+ctrl+up",
    "command": "runCommands",
    "args": { // x8
      "commands": ["cursorUpSelect", "cursorUpSelect", "cursorUpSelect", "cursorUpSelect", "cursorUpSelect", "cursorUpSelect", "cursorUpSelect", "cursorUpSelect"],
    },
    "when": "textInputFocus",
  },
  {
    "key": "ctrl+down",
    "command": "runCommands",
    "args": { // x8
      "commands": ["cursorDown", "cursorDown", "cursorDown", "cursorDown", "cursorDown", "cursorDown", "cursorDown", "cursorDown"],
    },
    "when": "textInputFocus",
  },
  {
    "key": "shift+ctrl+down",
    "command": "runCommands",
    "args": { // x8
      "commands": ["cursorDownSelect", "cursorDownSelect", "cursorDownSelect", "cursorDownSelect", "cursorDownSelect", "cursorDownSelect", "cursorDownSelect", "cursorDownSelect"],
    },
    "when": "textInputFocus",
  },
  {
    "key": "alt+left",
    "command": "editor.action.outdentLines",
    "when": "editorTextFocus",
  },
  {
    "key": "alt+right",
    "command": "editor.action.indentLines",
    "when": "editorTextFocus",
  },

  {
    "key": "ctrl+`",
    "command": "terminal.focus",
    "when": "!terminalFocus",
  },
  {
    "key": "ctrl+`",
    "command": "workbench.action.focusActiveEditorGroup",
    "when": "terminalFocus",
  },

  {
    "key": "ctrl+n",
    "command": "explorer.newFile",
  },
  {
    "key": "ctrl+shift+n",
    "command": "explorer.newFolder",
  },

  {
    "key": "ctrl+g ctrl+o",
    "command": "git.checkout",
  },
  {
    "key": "ctrl+g ctrl+n",
    "command": "git.branchFrom",
  },
  {
    "key": "ctrl+g ctrl+r",
    "command": "git.rebase",
  },
  {
    "key": "ctrl+g ctrl+p",
    "command": "git.push",
  },
  {
    "key": "ctrl+g shift+ctrl+p",
    "command": "git.publish",
  },
  {
    "key": "ctrl+g ctrl+u",
    "command": "git.pull",
  },
  {
    "key": "ctrl+g ctrl+c",
    "command": "git.commitStaged",
  },
  {
    "key": "ctrl+g ctrl+a",
    "command": "git.commitStagedAmend",
  },
  {
    "key": "ctrl+g ctrl+m",
    "command": "git.merge",
  },
  {
    "key": "ctrl+g ctrl+s",
    "command": "git.stash",
  },
  {
    "key": "ctrl+g ctrl+v",
    "command": "git.stashApply",
  },
  {
    "key": "ctrl+g ctrl+z",
    "command": "git.undoCommit",
  },
  {
    "key": "ctrl+g ctrl+]",
    "command": "git.stage",
  },
  {
    "key": "ctrl+g ctrl+[",
    "command": "git.unstage",
  },
  {
    "key": "ctrl+g ctrl+m",
    "command": "git.merge",
  },
  {
    "key": "ctrl+g ctrl+s",
    "command": "git.stash",
  },
  {
    "key": "ctrl+g ctrl+v",
    "command": "git.stashApply",
  },
  {
    "key": "ctrl+g ctrl+]",
    "command": "git.stage",
  },
  {
    "key": "ctrl+g ctrl+[",
    "command": "git.unstage",
  },
]