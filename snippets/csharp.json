{
  "for loop": {
    "prefix": "for",
    "body": [
      "for (int ${1:i} = 0; ${1:i} < ${2:length}; ++${1:i})",
      "{",
      "    $0",
      "}",
    ],
  },
  "Reverse for loop": {
    "prefix": "forr",
    "body": [
      "for (int ${2:length} - 1; ${1:i} >= 0; --${1:i})",
      "{",
      "    $0",
      "}",
    ],
  },

